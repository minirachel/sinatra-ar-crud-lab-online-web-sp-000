<h1>Edit An Article</h1>
<form action="/articles/<%= @article.id %>" method="POST">
    <input id="hidden" type="hidden" name="_method" value="PATCH">
    <p>Title: <input id="title" type="text" name="article[title]" value="<%= @article.title%>"></p>
    <p>Content: <input id="content" type="type" name="article[content]" value="<%= @article.content%>"></p>
    <input type="submit" id="submit">
</form>